{
  "name": "cv-frontend",
  "description": "My cv frontend",
  "version": "0.1.0",
  "engines": {
    "node": "20.x"
  },
  "private": true,
  "scripts": {
    "serve": "cd cv-frontend && npx vue-cli-service serve",
    "build": "cd cv-frontend && npx vue-cli-service build",
    "lint": "cd cv-frontend && npx vue-cli-service lint"
  },
  "dependencies": {
    "@vue/cli-service": "~5.0.0",
    "axios": "^1.7.5",
    "core-js": "^3.8.3",
    "vue": "^3.2.13",
    "vue-particles": "^1.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
